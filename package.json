{
  "name": "google-chat-to-slack",
  "version": "1.0.0",
  "description": "Migrates channels, threads, and messages, attachments and reactions from Google Chat to Slack.",
  "main": "dist/index.js",
  "bin": {
    "googletoslack": "./dist/bin/googletoslack.js"
  },
  "scripts": {
    "start": "ts-node bin/googletoslack.ts",
    "build": "tsc && chmod +x dist/bin/googletoslack.js",
    "dev": "ts-node bin/googletoslack.ts",
    "prepublishOnly": "pnpm run build",
    "prepare": "husky",
    "lint": "pnpm dlx ultracite lint",
    "format": "pnpm dlx ultracite format",
    "typecheck": "tsc --noEmit",
    "check": "pnpm dlx ultracite format && tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@napi-rs/keyring": "^1.1.8",
    "@slack/web-api": "^7.9.3",
    "cli-progress": "^3.12.0",
    "dotenv": "^17.2.0",
    "gaxios": "^7.1.1",
    "google-auth-library": "^10.1.0",
    "googleapis": "^152.0.0",
    "open": "^10.1.0",
    "yargs": "^18.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.1",
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^24.0.13",
    "@types/yargs": "^17.0.33",
    "husky": "^9.1.7",
    "memfs": "^4.17.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "ultracite": "5.0.35",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.3"
  },
  "keywords": [
    "slack",
    "google chat",
    "import",
    "migration",
    "cli",
    "google workspace"
  ],
  "author": "Markus Jura",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markusjura/google-chat-to-slack.git"
  },
  "homepage": "https://github.com/markusjura/google-chat-to-slack#readme",
  "bugs": {
    "url": "https://github.com/markusjura/google-chat-to-slack/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "packageManager": "pnpm@10.13.1"
}
