{
  "name": "chat-migrator",
  "version": "1.0.0",
  "description": "Node script to migrate channels, threads, and messages from Google Chat to Slack.",
  "main": "index.js",
  "scripts": {
    "start": "ts-node bin/chatmig.ts",
    "prepare": "husky",
    "lint": "pnpm dlx ultracite lint",
    "format": "pnpm dlx ultracite format",
    "typecheck": "tsc --noEmit",
    "check": "pnpm dlx ultracite format && tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@napi-rs/keyring": "^1.1.8",
    "@slack/web-api": "^7.9.3",
    "cli-progress": "^3.12.0",
    "dotenv": "^17.2.0",
    "gaxios": "^7.1.1",
    "google-auth-library": "^10.1.0",
    "googleapis": "^152.0.0",
    "open": "^10.1.0",
    "yargs": "^18.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.1",
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^24.0.13",
    "@types/yargs": "^17.0.33",
    "husky": "^9.1.7",
    "memfs": "^4.17.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "ultracite": "5.0.35",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.3"
  },
  "keywords": [
    "slack",
    "google chat",
    "import"
  ],
  "author": "Markus Jura",
  "packageManager": "pnpm@10.13.1"
}
